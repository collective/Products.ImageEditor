<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
	xmlns:i18n="http://xml.zope.org/namespaces/i18n">
	<object name="object" meta_type="CMF Action Category">
		<property name="title"></property>
		<object name="image_editor" meta_type="CMF Action"
			i18n:domain="Products.ImageEditor">
			<property name="title" i18n:translate="">Image Editor</property>
			<property name="description" i18n:translate=""></property>
			<property name="url_expr">string:${object_url}/@@imageeditor</property>
			<property name="icon_expr"></property>
			<property name="available_expr">object/@@image-editor-utility/editable</property>
			<property name="permissions">
				<element value="Products.ImageEditor.edit" />
			</property>
			<property name="visible">False</property>
		</object>
	</object>
</object>